# Prisma Data Proxy / Prisma Accelerate connection string
# Exemplo: prisma+postgres://accelerate.prisma-data.net/?api_key=xxxx
DATABASE_URL="prisma+postgres://YOUR_WORKSPACE.prisma-data.net/?api_key=YOUR_API_KEY"

# (Opcional) URL direta do banco para rodar migrações/acesso administrativo local
# DIRECT_DATABASE_URL="postgresql://USERNAME:PASSWORD@HOST/neondb?sslmode=require"

# JWT Secret - Use uma string segura e aleatória em produção
JWT_SECRET="changeme-super-secret-minimum-16-chars"

# Credenciais do administrador padrão (criado automaticamente no primeiro acesso)
DEFAULT_ADMIN_EMAIL="admin@motorotas.com"
DEFAULT_ADMIN_PASSWORD="Admin@123"
